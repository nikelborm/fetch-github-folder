{
  "name": "fetch-github-folder",
  "version": "0.1.6",
  "type": "module",
  "description": "Download any directory at any depth from any repo on GitHub on any git ref",
  "license": "MIT",
  "scripts": {
    "start": "tsc && node --env-file=.env ./dist/fetch-github-folder.js",
    "scratchpad": "tsc && node --env-file=.env ./dist/scratchpad.js",
    "test": "vitest --watch",
    "test-once": "vitest --run",
    "build": "./scripts/build.sh"
  },
  "dependencies": {
    "@effect/cli": "^0.51.0",
    "@effect/platform": "^0.72.0",
    "@effect/platform-node": "^0.68.0",
    "@octokit/core": "^6.1.3",
    "@octokit/openapi-types": "^22.2.0",
    "@octokit/request-error": "^6.1.6",
    "effect": "^3.12.0",
    "outdent": "^0.8.0",
    "tar-fs": "^3.0.6"
  },
  "devDependencies": {
    "@effect/vitest": "^0.16.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "^22.10.5",
    "@types/tar-fs": "^2.0.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.29.1",
    "rollup-plugin-visualizer": "^5.13.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nikelborm/fetch-github-folder.git"
  },
  "main": "./dist/src/index.js",
  "bin": {
    "fetch-github-folder": "dist/fetch-github-folder.js",
    "fgf": "dist/fetch-github-folder.js"
  },
  "bugs": {
    "url": "https://github.com/nikelborm/fetch-github-folder/issues",
    "email": "evadev@duck.com"
  },
  "files": [
    "dist",
    "!dist/minified",
    "!**/*.tsbuildinfo",
    ".github/FUNDING.yml",
    "template.env"
  ],
  "types": "./dist/src/index.d.ts",
  "homepage": "https://github.com/nikelborm/fetch-github-folder#readme",
  "exports": {
    ".": "./dist/src/index.js",
    "./index": "./dist/src/index.js",
    "./index.js": "./dist/src/index.js",
    "./package.json": "./package.json"
  },
  "keywords": [
    "github",
    "git",
    "octokit",
    "github-api",
    "folder",
    "scraper",
    "downloader",
    "download",
    "extract",
    "tarball",
    "targz",
    "directory"
  ],
  "author": {
    "name": "nikelborm",
    "email": "evadev@duck.com",
    "url": "https://github.com/nikelborm"
  },
  "contributors": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ],
  "maintainers": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ]
}

{
  "name": "fetch-github-folder",
  "version": "0.1.27",
  "type": "module",
  "description": "Download any directory at any depth from any repo on GitHub on any git ref",
  "license": "MIT",
  "scripts": {
    "start": "node --env-file=.env ./fetch-github-folder.ts",
    "scratchpad": "node --use-strict --env-file=.env ./scratchpad.ts",
    "test": "vitest --watch",
    "format": "prettier . --write",
    "coverage": "vitest run --coverage",
    "test-once": "vitest --run",
    "test-jsr": "tsc && jsr publish --dry-run",
    "build": "./scripts/build.sh",
    "doc": "./scripts/generate_docs.sh",
    "docs": "./scripts/generate_docs.sh",
    "watch-docs": "find src/ cli.ts index.ts errors.ts | entr -can ./scripts/generate_docs.sh"
  },
  "dependencies": {
    "@effect/cli": "^0.60.1",
    "@effect/platform": "^0.81.1",
    "@effect/platform-node": "^0.78.1",
    "@octokit/core": "^6.1.5",
    "@octokit/request-error": "^6.1.8",
    "effect": "^3.14.22",
    "outdent": "^0.8.0",
    "tar-fs": "^3.0.8",
    "tsafe": "^1.8.5",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@effect/language-service": "^0.12.1",
    "@effect/vitest": "^0.21.5",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^22.15.17",
    "@types/tar-fs": "^2.0.4",
    "@vitest/coverage-v8": "^3.1.3",
    "jsr": "^0.13.4",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nikelborm/fetch-github-folder.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/dts/index.d.ts",
  "bin": {
    "fetch-github-folder": "dist/fetch-github-folder.js",
    "fgf": "dist/fetch-github-folder.js"
  },
  "bugs": {
    "url": "https://github.com/nikelborm/fetch-github-folder/issues",
    "email": "evadev@duck.com"
  },
  "files": [
    "dist",
    "src",
    "!dist/minified",
    "!**/*.tsbuildinfo",
    "!**/*.spec.*",
    "!**/scratchpad.*",
    ".github/FUNDING.yml",
    "template.env",
    "cli.ts",
    "errors.ts",
    "index.ts"
  ],
  "homepage": "https://github.com/nikelborm/fetch-github-folder#readme",
  "exports": {
    ".": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/index.js"
    },
    "./index.js": {
      "types": "./dist/dts/index.d.ts",
      "default": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/dts/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./errors.js": {
      "types": "./dist/dts/errors.d.ts",
      "default": "./dist/errors.js"
    },
    "./cli": {
      "types": "./dist/dts/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./cli.js": {
      "types": "./dist/dts/cli.d.ts",
      "default": "./dist/cli.js"
    },
    "./package.json": "./package.json"
  },
  "keywords": [
    "github",
    "git",
    "octokit",
    "github-api",
    "folder",
    "scraper",
    "downloader",
    "download",
    "extract",
    "tarball",
    "targz",
    "directory"
  ],
  "author": {
    "name": "nikelborm",
    "email": "evadev@duck.com",
    "url": "https://github.com/nikelborm"
  },
  "contributors": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ],
  "maintainers": [
    {
      "name": "nikelborm",
      "email": "evadev@duck.com",
      "url": "https://github.com/nikelborm"
    }
  ]
}

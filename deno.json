{
  "$schema": "https://raw.githubusercontent.com/denoland/deno/refs/heads/main/cli/schemas/config-file.v1.json",
  "name": "@nikelborm/fetch-github-folder",
  "version": "0.1.28",
  "license": "MIT",
  "exports": {
    ".": "./index.ts",
    "./index": "./index.ts",
    "./index.ts": "./index.ts",
    "./errors": "./errors.ts",
    "./errors.ts": "./errors.ts",
    "./cli": "./cli.ts",
    "./cli.ts": "./cli.ts"
  },
  "publish": {
    "include": [
      ".github/FUNDING.yml",
      "template.env",
      "fetch-github-folder.ts",
      "src/**/*.ts",
      "index.ts",
      "errors.ts",
      "cli.ts",
      "README.md",
      "LICENSE"
    ],
    "exclude": ["**/*.spec.*", "scratchpad.*"]
  }
}

{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@nikelborm/fetch-github-folder",
  "version": "0.1.7",
  "exports": "./src/index.ts",
  "publish": {
    "include": [
      ".github/FUNDING.yml",
      "template.env",
      "src/**/*.ts",
      "README.md",
      "LICENSE"
    ],
    "exclude": [
      "**/*.spec.*",
      "scratchpad.*"
    ]
  }
}
